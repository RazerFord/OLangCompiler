class Animal is
  var weight : Real
  var height : Real

  this is
    this.weight = 0.0
    this.height = 0.0
  end
end

class Bird extends Animal is
  var wingspan : Real
  this is
    this.weight := 5.0
    this.height := 23.0
    this.wingspan := 1.2
  end
end

class Dog extends Animal is
  this is
    this.weight := 55.0
    this.height := 65.0
  end
end
