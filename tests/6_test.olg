class Array[T] is
    var Length: Integer
    this() is
    end
    method get(i: Integer): Integer is
       return i
    end
end

class Math is
  this() is
  end

  method min(arr: Array[Integer]): Integer is
    var min : Integer().Max()
    var i : Integer(0)
    while i.Less(arr.Length) loop
      min := this.min(i, arr.get(i))
      i := i.Plus(1)
    end
    return min
  end

  method min(a: Integer, b: Integer) : Integer is
    if a.Less(b) then
      return a
    else
      return b
    end
  end
end
