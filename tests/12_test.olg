class Base is
end

class A extends Base is
end

class Main is
  var success : false

  this() is
    var v1 : Base
    var v2 : A
    
    v1 := v2

    if this.test(v1).Equal(1) then success := true end
    if this.test(v2).Equal(2) then success := true end
  end

  method test(v: Base): Integer is
    return 1
  end

  method test(v: A): Integer is
    return 2
  end
end
