class Distance is
  method distance2(a: Point, b: Point): Real is
    var math : Math
    return math.pow2(a.x.Minus(b.x)).Plus(math.pow2(a.y.Minus(b.y)))
  end
end

class Math is
  this() is
  end
  method pow2(v: Real): Real is
    return v.Mult(v)
  end
end


