class Math is
  method min(a: Integer, b: Integer, c: Integer) : Integer is
    if a.Less(b) then
      if a.Less(c) then
        return a
      else 
        if c.Less(a) then
          return c
        end
      end
    else 
      if b.Less(c) then
        return b
      else 
        return c
      end 
    end
  end

  method min(a: Integer, b: Integer) : Integer is
    if a.Less(b) then
      return a
    else
      return b
    end
  end
end
