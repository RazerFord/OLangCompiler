class Math is
  this() is
  end

  method min(a: Integer, b: Integer) : Integer is
    var res: Integer()
    if a.Less(b).get() then
     res := a
    else
     res := b
    end
    return res
  end
end

class Main is
   method main() is
       var a: Integer(10)
       var b: Integer(12)
       var m: Math()
       printf("min %d\n", m.min(a, b).get())
   end
end